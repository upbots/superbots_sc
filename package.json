    "@openzeppelin/contracts": "4.3.3",
    "@openzeppelin/contracts-upgradeable": "4.3.3",
    "@openzeppelin/hardhat-upgrades": "^1.16.0",
    "abi-decoder": "^2.4.0",
    "axios": "^0.21.1",
    "commander": "^9.4.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.4.1",
    "node-fetch": "^3.2.4",
